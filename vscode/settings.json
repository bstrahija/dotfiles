{
    // Looks
    "workbench.colorTheme": "Nord",
    "workbench.iconTheme": "bearded-icons",
    "editor.fontFamily": "MonaspiceKr Nerd Font",
    "workbench.fontAliasing": "auto",
    "editor.fontSize": 12,
    "editor.fontWeight": "400",
    "editor.lineHeight": 2.6,
    "editor.cursorBlinking": "solid",
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "hidden",
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "window.zoomLevel": 0.2,
    "editor.rulers": [
        120
    ],
    "workbench.editor.editorActionsLocation": "titleBar",
    "editor.inlayHints.enabled": "off",
    "breadcrumbs.enabled": false,
	"workbench.editor.tabSizing": "fit",
    "catppuccin-icons.hidesExplorerArrows": true,

    // Project pane (file explorer)
    "workbench.tree.indent": 18,
    "files.exclude": {
        "**/.idea": true
    },

    // Search
    "search.mode": "reuseEditor",
    "search.exclude": {
        "**/.git": true,
        "**/*.log": true,
        "**/node_modules": true,
        "**/tmp": true,
        "**/wp-content/**/public": true
    },

    // AI
    "github.copilot.nextEditSuggestions.enabled": true,
    "editor.inlineSuggest.enabled": true,

    // LSP's
    "editor.codeLens": false,
    "editor.formatOnSave": true,

    // ==> PHP
    "php.version": "v8.4",
    "intelephense.environment.phpVersion": "8.4.0",
    "php.codeLens.enabled": false,
    "php.executables": {
        "v7.4": "/Users/bstrahija/Library/Application Support/Herd/bin/php74",
        "v8.0": "/Users/bstrahija/Library/Application Support/Herd/bin/php80",
        "v8.1": "/Users/bstrahija/Library/Application Support/Herd/bin/php81",
        "v8.2": "/Users/bstrahija/Library/Application Support/Herd/bin/php82",
        "v8.3": "/Users/bstrahija/Library/Application Support/Herd/bin/php83",
        "v8.4": "/Users/bstrahija/Library/Application Support/Herd/bin/php84",
        "v8.5": "/Users/bstrahija/Library/Application Support/Herd/bin/php85"
    },
    "laravel-pint.enable": true,
    "intelephense.files.associations": ["*.blade.php", "*.php", "*.phtml"],
    "Laravel.phpEnvironment": "herd",
    "[php]": {
        "editor.defaultFormatter": "DEVSENSE.phptools-vscode",
        "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\"-,.<>/?"
    },
    "[blade]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "DEVSENSE.phptools-vscode",
    },
    "php.suggest.basic": false,
    "php.validate.enable": false,
    "php.problems.exclude": {
        "**/*": [
            "PHP6602"
        ]
    },

    // ==> Other LSP's
    "json.format.enable": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    // Git
    "gitblame.inlineMessageEnabled": true,

    // Other
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 300,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "editor.linkedEditing": true,
    "editor.semanticTokenColorCustomizations": {},

    // Spell Check
    "cSpell.blockCheckingWhenLineLengthGreaterThan": 100000,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.customDictionaries": {
        "custom-dictionary-user": {
            "name": "My Dictionary",
            "path": "~/.config/cspell/dictionary.txt",
            "addWords": true,
            "scope": "user"
        }
    },

    // Vim
    "editor.lineNumbers": "relative",
    "vim.leader": "<space>",
    "vim.incsearch": true,
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.highlightedyank.enable": true,
    "vim.smartRelativeLine": true,
    "vim.surround": true,
    "vim.easymotion": true, // Search with "\\f"
    "extensions.experimental.affinity": { // Vim - make if more performant
        "vscode.vim": 1,
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-textobjects.vimEnabled": true, // Vim text objects

    // Vim keybindings

    // ==> Insert mode
    "vim.insertModeKeyBindings": [
        { "before": ["j", "j"], "after": ["<Esc>"], "commands": [":nohl"], "silent": true },
        { "before": ["j", "k"], "after": ["<Esc>"], "commands": [":nohl"], "silent": true },
    ],

     // ==> Normal mode
    "vim.normalModeKeyBindings": [
        { "before": [":"],     "commands": ["workbench.action.showCommands"], "silent": true },
        { "before": ["<C-n>"], "commands": [":nohl"] },

        // Move to line start/end
        { "before": ["L"], "after": ["$"] },
        { "before": ["H"], "after": ["^"] },
        { "before": ["0"], "after": ["^"] },
        { "before": ["-"], "after": ["$"] },
        { "before": ["B"], "after": ["^"] },

        // Copy until end of line
        { "before": ["Y"], "after": ["y", "$"] },

        // Text object motions
        // ===> Selection
        { "before": ["v", "a", "f"], "commands": ["vscode-textobjects.select.next.outer.function.start"] },
        { "before": ["v", "i", "f"], "commands": ["vscode-textobjects.select.next.inner.function.start"] },
        { "before": ["v", "a", "l"], "commands": ["vscode-textobjects.select.next.outer.loop.start"] },
        { "before": ["v", "i", "l"], "commands": ["vscode-textobjects.select.next.inner.loop.start"] },
        { "before": ["v", "a", "i"], "commands": ["vscode-textobjects.select.next.outer.conditional.start"] },
        { "before": ["v", "i", "i"], "commands": ["vscode-textobjects.select.next.inner.conditional.start"] },
        { "before": ["v", "a", "r"], "commands": ["vscode-textobjects.select.next.outer.rhs.start"] },
        { "before": ["v", "i", "r"], "commands": ["vscode-textobjects.select.next.inner.rhs.start"] },
        { "before": ["v", "a", "h"], "commands": ["vscode-textobjects.select.next.outer.lhs.start"] },
        { "before": ["v", "i", "h"], "commands": ["vscode-textobjects.select.next.inner.lhs.start"] },
        { "before": ["v", "a", "v"], "commands": ["vscode-textobjects.select.next.outer.variable.start"] },
        { "before": ["v", "a", "q"], "commands": ["vscode-textobjects.select.next.outer.string.start"] },
        { "before": ["v", "i", "q"], "commands": ["vscode-textobjects.select.next.inner.string.start"] },
        { "before": ["v", "a", "k"], "commands": ["vscode-textobjects.select.next.outer.class.start"] },
        { "before": ["v", "i", "k"], "commands": ["vscode-textobjects.select.next.inner.class.start"] },
        { "before": ["v", "a", "a"], "commands": ["vscode-textobjects.select.next.outer.array.start"] },
        { "before": ["v", "i", "a"], "commands": ["vscode-textobjects.select.next.inner.array.start"] },
        { "before": ["v", "a", "o"], "commands": ["vscode-textobjects.select.next.outer.object.start"] },
        { "before": ["v", "i", "o"], "commands": ["vscode-textobjects.select.next.inner.object.start"] },
        { "before": ["v", "a", "e"], "commands": ["vscode-textobjects.select.next.outer.parameters.start"] },
        { "before": ["v", "i", "e"], "commands": ["vscode-textobjects.select.next.inner.parameters.start"] },
        { "before": ["v", "a", "m"], "commands": ["vscode-textobjects.select.next.outer.call.start"] },
        { "before": ["v", "i", "m"], "commands": ["vscode-textobjects.select.next.inner.call.start"] },
        { "before": ["v", "a", "t"], "commands": ["vscode-textobjects.select.next.outer.type.start"] },
        { "before": ["v", "i", "t"], "commands": ["vscode-textobjects.select.next.inner.type.start"] },
        { "before": ["v", "a", "c"], "commands": ["vscode-textobjects.select.next.outer.comment.start"] },
        { "before": ["v", "i", "c"], "commands": ["vscode-textobjects.select.next.inner.comment.start"] },

        // ==> Moving
        { "before": ["[", "f"], "commands": ["vscode-textobjects.goTo.next.outer.function.start"] },
        { "before": ["[", "F"], "commands": ["vscode-textobjects.goTo.next.inner.function.start"] },
        { "before": ["[", "q"], "commands": ["vscode-textobjects.goTo.next.outer.string.start"] },
        { "before": ["[", "Q"], "commands": ["vscode-textobjects.goTo.next.inner.string.start"] },
        { "before": ["]", "f"], "commands": ["vscode-textobjects.goTo.previous.outer.function.start"] },
        { "before": ["]", "F"], "commands": ["vscode-textobjects.goTo.previous.inner.function.start"] },
        { "before": ["]", "q"], "commands": ["vscode-textobjects.goTo.previous.outer.string.start"] },
        { "before": ["]", "Q"], "commands": ["vscode-textobjects.goTo.previous.inner.string.start"] },

        // ==> Deleting
        { "before": ["d", "a", "f"], "commands": ["vscode-textobjects.delete.next.outer.function.start"] },
        { "before": ["d", "i", "f"], "commands": ["vscode-textobjects.delete.next.inner.function.start"] },
        { "before": ["d", "a", "l"], "commands": ["vscode-textobjects.delete.next.outer.loop.start"] },
        { "before": ["d", "i", "l"], "commands": ["vscode-textobjects.delete.next.inner.loop.start"] },
        { "before": ["d", "a", "i"], "commands": ["vscode-textobjects.delete.next.outer.conditional.start"] },
        { "before": ["d", "i", "i"], "commands": ["vscode-textobjects.delete.next.inner.conditional.start"] },
        { "before": ["d", "a", "r"], "commands": ["vscode-textobjects.delete.next.outer.rhs.start"] },
        { "before": ["d", "i", "r"], "commands": ["vscode-textobjects.delete.next.inner.rhs.start"] },
        { "before": ["d", "a", "h"], "commands": ["vscode-textobjects.delete.next.outer.lhs.start"] },
        { "before": ["d", "i", "h"], "commands": ["vscode-textobjects.delete.next.inner.lhs.start"] },
        { "before": ["d", "a", "v"], "commands": ["vscode-textobjects.delete.next.outer.variable.start"] },
        { "before": ["d", "a", "q"], "commands": ["vscode-textobjects.delete.next.outer.string.start"] },
        { "before": ["d", "i", "q"], "commands": ["vscode-textobjects.delete.next.inner.string.start"] },
        { "before": ["d", "a", "k"], "commands": ["vscode-textobjects.delete.next.outer.class.start"] },
        { "before": ["d", "i", "k"], "commands": ["vscode-textobjects.delete.next.inner.class.start"] },
        { "before": ["d", "a", "a"], "commands": ["vscode-textobjects.delete.next.outer.array.start"] },
        { "before": ["d", "i", "a"], "commands": ["vscode-textobjects.delete.next.inner.array.start"] },
        { "before": ["d", "a", "o"], "commands": ["vscode-textobjects.delete.next.outer.object.start"] },
        { "before": ["d", "i", "o"], "commands": ["vscode-textobjects.delete.next.inner.object.start"] },
        { "before": ["d", "a", "e"], "commands": ["vscode-textobjects.delete.next.outer.parameters.start"] },
        { "before": ["d", "i", "e"], "commands": ["vscode-textobjects.delete.next.inner.parameters.start"] },
        { "before": ["d", "a", "m"], "commands": ["vscode-textobjects.delete.next.outer.call.start"] },
        { "before": ["d", "i", "m"], "commands": ["vscode-textobjects.delete.next.inner.call.start"] },
        { "before": ["d", "a", "t"], "commands": ["vscode-textobjects.delete.next.outer.type.start"] },
        { "before": ["d", "i", "t"], "commands": ["vscode-textobjects.delete.next.inner.type.start"] },
        { "before": ["d", "a", "c"], "commands": ["vscode-textobjects.delete.next.outer.comment.start"] },
        { "before": ["d", "i", "c"], "commands": ["vscode-textobjects.delete.next.inner.comment.start"] },
        { "before": ["d", "a", "n"], "commands": ["vscode-textobjects.delete.next.outer.node.start"] },

        // ==> Yanking (copying)
        { "before": ["y", "a", "f"], "commands": ["vscode-textobjects.yank.next.outer.function.start"] },
        { "before": ["y", "i", "f"], "commands": ["vscode-textobjects.yank.next.inner.function.start"] },
        { "before": ["y", "a", "l"], "commands": ["vscode-textobjects.yank.next.outer.loop.start"] },
        { "before": ["y", "i", "l"], "commands": ["vscode-textobjects.yank.next.inner.loop.start"] },
        { "before": ["y", "a", "i"], "commands": ["vscode-textobjects.yank.next.outer.conditional.start"] },
        { "before": ["y", "i", "i"], "commands": ["vscode-textobjects.yank.next.inner.conditional.start"] },
        { "before": ["y", "a", "r"], "commands": ["vscode-textobjects.yank.next.outer.rhs.start"] },
        { "before": ["y", "i", "r"], "commands": ["vscode-textobjects.yank.next.inner.rhs.start"] },
        { "before": ["y", "a", "h"], "commands": ["vscode-textobjects.yank.next.outer.lhs.start"] },
        { "before": ["y", "i", "h"], "commands": ["vscode-textobjects.yank.next.inner.lhs.start"] },
        { "before": ["y", "a", "v"], "commands": ["vscode-textobjects.yank.next.outer.variable.start"] },
        { "before": ["y", "a", "q"], "commands": ["vscode-textobjects.yank.next.outer.string.start"] },
        { "before": ["y", "i", "q"], "commands": ["vscode-textobjects.yank.next.inner.string.start"] },
        { "before": ["y", "a", "k"], "commands": ["vscode-textobjects.yank.next.outer.class.start"] },
        { "before": ["y", "i", "k"], "commands": ["vscode-textobjects.yank.next.inner.class.start"] },
        { "before": ["y", "a", "a"], "commands": ["vscode-textobjects.yank.next.outer.array.start"] },
        { "before": ["y", "i", "a"], "commands": ["vscode-textobjects.yank.next.inner.array.start"] },
        { "before": ["y", "a", "o"], "commands": ["vscode-textobjects.yank.next.outer.object.start"] },
        { "before": ["y", "i", "o"], "commands": ["vscode-textobjects.yank.next.inner.object.start"] },
        { "before": ["y", "a", "e"], "commands": ["vscode-textobjects.yank.next.outer.parameters.start"] },
        { "before": ["y", "i", "e"], "commands": ["vscode-textobjects.yank.next.inner.parameters.start"] },
        { "before": ["y", "a", "m"], "commands": ["vscode-textobjects.yank.next.outer.call.start"] },
        { "before": ["y", "i", "m"], "commands": ["vscode-textobjects.yank.next.inner.call.start"] },
        { "before": ["y", "a", "t"], "commands": ["vscode-textobjects.yank.next.outer.type.start"] },
        { "before": ["y", "i", "t"], "commands": ["vscode-textobjects.yank.next.inner.type.start"] },
        { "before": ["y", "a", "c"], "commands": ["vscode-textobjects.yank.next.outer.comment.start"] },
        { "before": ["y", "i", "c"], "commands": ["vscode-textobjects.yank.next.inner.comment.start"] },
        { "before": ["y", "a", "n"], "commands": ["vscode-textobjects.yank.next.outer.node.start"] },
        { "before": ["y", "i", "n"], "commands": ["vscode-textobjects.yank.next.inner.node.start"] },

        // ==> Changing (replacing)
        { "before": ["c", "a", "f"], "commands": ["vscode-textobjects.change.next.outer.function.start"] },
        { "before": ["c", "i", "f"], "commands": ["vscode-textobjects.change.next.inner.function.start"] },
        { "before": ["c", "a", "l"], "commands": ["vscode-textobjects.change.next.outer.loop.start"] },
        { "before": ["c", "i", "l"], "commands": ["vscode-textobjects.change.next.inner.loop.start"] },
        { "before": ["c", "a", "i"], "commands": ["vscode-textobjects.change.next.outer.conditional.start"] },
        { "before": ["c", "i", "i"], "commands": ["vscode-textobjects.change.next.inner.conditional.start"] },
        { "before": ["c", "a", "r"], "commands": ["vscode-textobjects.change.next.outer.rhs.start"] },
        { "before": ["c", "i", "r"], "commands": ["vscode-textobjects.change.next.inner.rhs.start"] },
        { "before": ["c", "a", "h"], "commands": ["vscode-textobjects.change.next.outer.lhs.start"] },
        { "before": ["c", "i", "h"], "commands": ["vscode-textobjects.change.next.inner.lhs.start"] },
        { "before": ["c", "a", "v"], "commands": ["vscode-textobjects.change.next.outer.variable.start"] },
        { "before": ["c", "a", "q"], "commands": ["vscode-textobjects.change.next.outer.string.start"] },
        { "before": ["c", "i", "q"], "commands": ["vscode-textobjects.change.next.inner.string.start"] },
        { "before": ["c", "a", "k"], "commands": ["vscode-textobjects.change.next.outer.class.start"] },
        { "before": ["c", "i", "k"], "commands": ["vscode-textobjects.change.next.inner.class.start"] },
        { "before": ["c", "a", "a"], "commands": ["vscode-textobjects.change.next.outer.array.start"] },
        { "before": ["c", "i", "a"], "commands": ["vscode-textobjects.change.next.inner.array.start"] },
        { "before": ["c", "a", "o"], "commands": ["vscode-textobjects.change.next.outer.object.start"] },
        { "before": ["c", "i", "o"], "commands": ["vscode-textobjects.change.next.inner.object.start"] },
        { "before": ["c", "a", "e"], "commands": ["vscode-textobjects.change.next.outer.parameters.start"] },
        { "before": ["c", "i", "e"], "commands": ["vscode-textobjects.change.next.inner.parameters.start"] },
        { "before": ["c", "a", "m"], "commands": ["vscode-textobjects.change.next.outer.call.start"] },
