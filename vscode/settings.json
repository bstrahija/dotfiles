        { "before": ["d", "i", "c"], "commands": ["vscode-textobjects.delete.next.inner.comment.start"] },
        { "before": ["d", "a", "n"], "commands": ["vscode-textobjects.delete.next.outer.node.start"] },

        // ==> Yanking (copying)
        { "before": ["y", "a", "f"], "commands": ["vscode-textobjects.yank.next.outer.function.start"] },
        { "before": ["y", "i", "f"], "commands": ["vscode-textobjects.yank.next.inner.function.start"] },
        { "before": ["y", "a", "l"], "commands": ["vscode-textobjects.yank.next.outer.loop.start"] },
        { "before": ["y", "i", "l"], "commands": ["vscode-textobjects.yank.next.inner.loop.start"] },
        { "before": ["y", "a", "i"], "commands": ["vscode-textobjects.yank.next.outer.conditional.start"] },
        { "before": ["y", "i", "i"], "commands": ["vscode-textobjects.yank.next.inner.conditional.start"] },
        { "before": ["y", "a", "r"], "commands": ["vscode-textobjects.yank.next.outer.rhs.start"] },
        { "before": ["y", "i", "r"], "commands": ["vscode-textobjects.yank.next.inner.rhs.start"] },
        { "before": ["y", "a", "h"], "commands": ["vscode-textobjects.yank.next.outer.lhs.start"] },
        { "before": ["y", "i", "h"], "commands": ["vscode-textobjects.yank.next.inner.lhs.start"] },
